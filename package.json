{
  "name": "hotels-backend",
  "version": "1.0.0",
  "description": "Contains models and api for managing hotels",
  "main": "index.js",
  "scripts": {
    "lint": "eslint \"src/**/*.{ts,js}\"",
    "copy-files": "./node_modules/.bin/cpx \"src/**/*.{json,html,csv}\" dist",
    "tsc": "./node_modules/.bin/tsc",
    "build": "npm run clean && npm run tsc && npm run copy-files",
    "clean": "rm -rf dist",
    "start": "node dist/index.js",
    "test": "jest",
    "migrate:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "migrate:up": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migrate:down": "ts-node ./node_modules/typeorm/cli.js migration:revert"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smithaitufe/hotels-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/smithaitufe/hotels-backend/issues"
  },
  "homepage": "https://github.com/smithaitufe/hotels-backend#readme",
  "dependencies": {
    "cpx": "^1.5.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^9.1.1",
    "typeorm": "^0.2.29",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/pg": "^7.14.7",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4"
  }
}
